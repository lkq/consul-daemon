docker run -d -h consul-node2 -v /mnt:/data  \
    -p 192.168.33.61:8300:8300 \
    -p 192.168.33.61:8301:8301 \
    -p 192.168.33.61:8301:8301/udp \
    -p 192.168.33.61:8302:8302 \
    -p 192.168.33.61:8302:8302/udp \
